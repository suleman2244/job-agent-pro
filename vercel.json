{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/dist/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/dist/index.html"
        }
    ]
}