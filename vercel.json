{
    "version": 2,
    "buildCommand": "cd web-app && npm install && npm run build",
    "outputDirectory": "dist",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}